<%= form_with(model: @address) do |f| %>

  <fieldset>
    <%= f.label :street_number %><br />
    <%= f.text_field :street_number %>
  </fieldset>
  <fieldset>
    <%= f.label :street_name %><br />
    <%= f.text_field :street_name %>
  </fieldset>
  <fieldset>
    <%= f.label :suburb %><br />
    <%= f.text_field :suburb %>
  </fieldset>
  <fieldset>
    <%= f.label :postcode %><br />
    <%= f.number_field :postcode %>
  </fieldset>
  <fieldset>
    <%= f.label :state %><br />
    <%= f.text_field :state %>
  </fieldset>
  <fieldset>
    <%= f.submit class: 'button is-primary' %>
  </fieldset>
<% end %>