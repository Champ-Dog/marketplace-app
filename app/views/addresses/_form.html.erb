<%= form_with(model: [@profile, @address]) do |f| %>
  <ul> 
    <% @address.errors.full_messages.each do |error| %>
      <li><%= error %></li>
    <% end %>
  </ul>
  <p>
    <%= f.label :street_number %><br />
    <%= f.text_field :street_number %>
  </p>
  <p>
    <%= f.label :street_name %><br />
    <%= f.text_field :street_name %>
  </p>
  <p>
    <%= f.label :suburb %><br />
    <%= f.text_field :suburb %>
  </p>
  <p>
    <%= f.label :postcode %><br />
    <%= f.number_field :postcode %>
  </p>
  <p>
    <%= f.label :state %><br />
    <%= f.text_field :state %>
  </p>
  <p>
    <%= f.submit class: 'button is-primary' %>
  </p>
<% end %>